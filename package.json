{
  "name": "amd-bundle",
  "version": "1.3.2",
  "description": "A dual-use tool for bundling up AMD modules, which is much easier to use than r.js with Almond or AMDclean",
  "main": "build/Package.js",
  "bin": "build/index.js",
  "engines": {
    "node": "^6.5.0"
  },
  "scripts": {
    "lint": "eslint index.js source/ test/ --fix",
    "prepublishOnly": "npm run lint  &&  babel source/ --out-dir build/",
    "build": "npm run prepublishOnly  &&  esdoc  &&  git add .",
    "start": "web-server docs/ -o",
    "install": "node install",
    "check": "mocha -r babel-register -r babel-polyfill -r should -c --inline-diffs",
    "test": "npm run check -- --exit",
    "debug": "npm run check -- --inspect-brk",
    "precommit": "npm run lint  &&  npm test  &&  git add ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TechQuery/AMD_bundle.js.git"
  },
  "keywords": [
    "javascript",
    "amd-loader",
    "bundle",
    "require-js",
    "command-line"
  ],
  "author": "shiy007@qq.com",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/TechQuery/AMD_bundle.js/issues"
  },
  "homepage": "https://techquery.github.io/AMD_bundle.js/",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "commander": "^2.11.0",
    "fs-extra": "^6.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "esdoc": "^1.1.0",
    "esdoc-external-nodejs-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.19.1",
    "husky": "^0.14.3",
    "koapache": "^0.2.3",
    "mocha": "^5.2.0",
    "should": "^13.2.1"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-runtime"
    ]
  }
}
