{
  "name": "amd-bundle",
  "version": "1.0.0",
  "description": "A dual-use tool for bundling up AMD modules, which is much easier to use than r.js with Almond or AMDclean",
  "main": "libs/Package.js",
  "bin": "index.js",
  "engines": {
    "node": "^6.5.0"
  },
  "scripts": {
    "lint": "eslint index.js libs/ test/",
    "install": "echo exports.test = 1;> node_modules/test.js",
    "test": "mocha -r babel-register -r babel-polyfill -r should --no-timeouts -c --inline-diffs --exit",
    "debug": "npm test -- --inspect-brk",
    "build": "esdoc",
    "start": "web-server docs/ -o",
    "precommit": "npm run lint  &&  npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TechQuery/AMD_bundle.js.git"
  },
  "keywords": [
    "javascript",
    "amd-loader",
    "bundle",
    "require-js",
    "command-line"
  ],
  "author": "shiy007@qq.com",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/TechQuery/AMD_bundle.js/issues"
  },
  "homepage": "https://techquery.github.io/AMD_bundle.js/",
  "dependencies": {
    "babel-env": "^2.4.1",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "commander": "^2.11.0",
    "fs-extra": "^6.0.1"
  },
  "devDependencies": {
    "esdoc": "^1.1.0",
    "esdoc-external-nodejs-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.19.1",
    "husky": "^0.14.3",
    "koapache": "^0.2.2",
    "mocha": "^5.1.1",
    "should": "^13.2.1"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
